<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>1.0.0.0</Version>
        <Authors>SoftwareCraft</Authors>
        <Description>SoftwareCraft.Forge is the base library for the entire SoftwareCraft universe.</Description>
        <PackageProjectUrl>https://github.com/zyrconium13/SoftwareCraft.Forge</PackageProjectUrl>
        <RepositoryUrl>https://github.com/zyrconium13/SoftwareCraft.Forge</RepositoryUrl>
        <PackageTags>c-sharp functional-programming dotnet core standard</PackageTags>
        <PackageReleaseNotes>Contains only the Unit type for now.</PackageReleaseNotes>
        <Company>SoftwareCraft</Company>
        <AssemblyVersion>1.0.0.0</AssemblyVersion>
        <FileVersion>1.0.0.0</FileVersion>
        <RootNamespace>SoftwareCraft.Functional</RootNamespace>
    </PropertyGroup>

</Project>
